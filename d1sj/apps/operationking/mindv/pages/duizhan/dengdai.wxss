.pipei_cg  {
  opacity:1;
  position: absolute;
  top:0;
}
.checked{
  animation:bouncew 1s .10s ease both;
  animation-fill-mode: forwards;
}

@keyframes bouncew {
0% {
opacity:0;
-webkit-transform:scale(.3)
}
50% {
opacity:1;
-webkit-transform:scale(1)
}
70% {
-webkit-transform:scale(1)
}
100% {
  opacity:1;
-webkit-transform:scale(1)
}
}

