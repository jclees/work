.star1 {
  width: 1px;
  height: 1px;
  z-index: 0;
  display: block;
  position: absolute;
  top: 50rpx;
  right: 800rpx;
  border-radius: 4px;
  animation: star1 2s infinite linear;
  opacity: 0;
}

.star1:after {
  position: relative;
  display: block;
  top: -49px;
  left: 23px;
  border-left: 1rpx solid #fff;
  border-width: 0 0 0 1rpx;
  width: 100px;
  height: 100px;
  opacity: 0.3;
  content: "";
  transform: rotate(45deg);
}

@keyframes star1 {
  0% {
    opacity: 0;
    transform: scale(0) translate(0, 0);
  }

  50% {
    opacity: 1;
    transform: scale(1) translate(-400px, 400px, 400px, 400px);
  }

  100% {
    opacity: 0;
    transform: scale(1) translate(-400px, 400px);
  }
}

