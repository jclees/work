page {
  background: #f6f6f6;
}

.contact_main .mine_user {
  overflow: hidden;
}

.contact_main .mine_user .user_bg {
  width: 100%;
}

.contact_main .mine_user .user_main {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
}

.contact_main .mine_user .user_img {
  width: 100rpx;
  height: 100rpx;
  padding: 20rpx;
  border-radius: 100rpx;
  margin: 60rpx auto 14rpx;
  border: 1px solid #fff;
}

.contact_main .mine_user .user_name {
  font-size: 28rpx;
  color: #fff;
  text-align: center;
  margin: 0 0 24rpx 0;
}

.contact_main .mine_user .user_btns {
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 0 24rpx 0;
}

.contact_main .mine_user .user_btns > view:nth-child(1) {
  background: #f67a1b;
  border-radius: 8rpx;
  height: 48rpx;
  line-height: 48rpx;
  padding: 0 30rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  margin-right: 90rpx;
}

.contact_main .mine_user .user_btns > view:nth-child(2) {
  display: flex;
  align-items: center;
  justify-content: center;
}

.contact_main .mine_user .user_btns > view:nth-child(2) > view:nth-child(1) {
  background: #1caa1b;
  border-radius: 8rpx;
  height: 48rpx;
  line-height: 48rpx;
  padding: 0 30rpx;
  font-size: 28rpx;
  color: #fff;
  margin-right: 30rpx;
}

.contact_main .mine_user .user_btns > view:nth-child(2) > image {
  width: 36rpx;
}

.contact_main .mine_list {
  overflow: hidden;
  margin-top: 20rpx;
}

.contact_main .mine_list .items {
  margin-bottom: 20rpx;
  background: #fff;
  padding: 0 40rpx;
  position: relative;
}

.contact_main .mine_list .items .cut_bg {
  width: 100%;
  position: absolute;
  bottom: -70rpx;
  left: 0;
  display: flex;
  padding: 0 68rpx;
  z-index: 2;
  justify-content: space-between;
}

.contact_main .mine_list .items .cut_bg > image {
  width: 22rpx;
}

.contact_main .mine_list .items.items_two {
  background: transparent;
  border: 1px solid #bfbfbf;
  margin: 0 40rpx;
  padding: 0 20rpx;
  border-radius: 20rpx;
}

.contact_main .mine_list .item_tit {
  font-size: 34rpx;
  position: relative;
  padding: 20rpx;
  margin-left: 40rpx;
}

.contact_main .mine_list .items > textarea {
  color: #000;
  width: 100%;
  padding: 10rpx 0;
  font-size: 26rpx;
}

