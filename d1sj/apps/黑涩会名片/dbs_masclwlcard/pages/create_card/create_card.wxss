.contact_main .mine_list .item_tit::after {
  content: "";
  position: absolute;
  height: 34rpx;
  left: 0;
  top: 24rpx;
  width: 6rpx;
  background: #f67a1b;
}

.sava_btn {
  text-align: center;
}

.sava_btn button {
  width: calc(100% - 80rpx);
  height: 88rpx;
  line-height: 88rpx;
  margin-bottom: 88rpx;
  margin-top: 66rpx;
  color: #fff;
  border-radius: 8rpx;
  background: #1caa1b;
}

.contact_main .mine_list .item_con .from_info {
  display: flex;
  padding: 30rpx 0;
}

.contact_main .mine_list .item_con .from_info .inp_box {
  flex: 1;
}
.contact_main .mine_list .item_con .from_info .img_box .img_prev {
  width: 180rpx;
  height: 180rpx;
  background: #f6f6f6;
  margin-right: 20rpx;
  position: relative;
}
.contact_main .mine_list .item_con .from_info .img_box .img_prev .upload_tips {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  font-size: 28rpx;
  color: #1caa1b;
  width: 100%;
  text-align: center;
}
.contact_main .mine_list .item_con .from_info .img_box .img_prev .cool {
  font-size: 26rpx;
  color: #bfbfbf;
}

.contact_main .mine_list .item_con .from_info .img_box > .upload_txt{
  font-size: 20rpx;
  color: #999;
  padding-top: 10rpx;
}


.contact_main .mine_list .item_con .from_info > .inp_box input {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 8rpx;
  border-bottom: 1px solid #efefef;
  font-size: 30rpx;
  color: #000;
  padding-left: 10rpx;
}
.contact_main .mine_list .item_con .from_info > .inp_box input:last-of-type{
  border:0;
}

.contact_main .mine_list .item_con .from_info .many {
  display: flex;
  width: 380rpx;
}

.contact_main .mine_list .item_con .from_info .many input {
  margin-right: 30rpx;
}

.contact_main .mine_list .item_con .more_from_info {
  border-top: 1px solid #bfbfbf;
  padding-top: 10rpx;
}

.contact_main .mine_list .item_con .more_from_info .more_from_info_items {
  border-bottom: 1px solid #efefef;
  padding: 16rpx 0;
  display: flex;
  align-items: center;
}

.contact_main .mine_list .item_con .more_from_info .more_from_info_items:last-of-type {
  border: 0;
}

.contact_main .mine_list .item_con .more_from_info .more_from_info_items .more_from_info_tit {
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: #1caa1b;
  margin-left: 10rpx;
  width: 200rpx;
}

.contact_main .mine_list .item_con .more_from_info .more_from_info_items .more_from_info_tit image {
  width: 40rpx;
  height: 40rpx;
  border-radius: 100%;
  margin-right: 16rpx;
}

.contact_main .mine_list .item_con .more_from_info .more_from_info_items input {
  font-size: 30rpx;
  color: #3c3c3c;
  margin-left: 10rpx;
  flex: 1;
  text-align: right;
}

.contact_main .pop-content >.uoload_imgs {
  margin: 0 0 30rpx;
  align-items: flex-start;
  height: 320rpx;
  width: 100%;
  padding: 20rpx;
  position: relative;
  border: 2px dashed #efefef;
}
.from_img_box {
  padding: 20rpx 0;
}

.from_img_box .imgs_box {
  position: relative;
}

.from_img_box .imgs_box > image {
  width: 100%;
}

.from_img_box .imgs_box .del_img {
  position: absolute;
  right: -15rpx;
  top: -15rpx;
  width: 36rpx;
  height: 36rpx;
}

.from_img_box .imgs_box .del_img image {
  width: 36rpx;
  height: 36rpx;
}

.from_img_box .choose_box {
  /* position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%); */
  text-align: center;
  color:#bfbfbf;
  font-size: 26rpx;
  padding: 60rpx 0;
}

.from_img_box .choose_box >image {
  width: 84rpx;
  height: auto;
  margin: 10rpx auto;
}

