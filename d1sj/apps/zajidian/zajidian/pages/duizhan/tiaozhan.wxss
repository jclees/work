.ope {
  position: absolute;
  z-index: 0;
  top:80rpx;
  width: 100%;
  height: auto;
  text-align: center;
  animation: rotationx 20s linear infinite;
}
.ope image {
  width: 680rpx;
  height: 680rpx;
}

@-webkit-keyframes rotationx{
    from {transform: rotate(0deg);}
    to {transform: rotate(360deg);}
}

.yanc2 {
    animation:bounceT 1s .2s ease both;
    animation-delay:0s;
}
@keyframes bounceT {
0% {
opacity:0;
-webkit-transform:scale(2)
}
50% {
opacity:1;
-webkit-transform:scale(1)
}
70% {
-webkit-transform:scale(1)
}
100% {
-webkit-transform:scale(1)
}
}



