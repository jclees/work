checkbox .wx-checkbox-input {
  border:1px solid #c1bfcd;
  border-radius: 8rpx;
  width: 32rpx;
  height: 32rpx;
}

checkbox .wx-checkbox-input-checked::before {
  background-color: #322c5b;
  color: #fff;
  padding:0;
  border:1px solid #322c5b;
  border-radius: 8rpx;
  width: 32rpx;
  height: 32rpx;
}

checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
font:normal normal normal 14px/1 "weui";
content:"\EA08";
font-size:26rpx;
position:absolute;
top:50%;
left:50%;
text-align: center;
line-height: 32rpx;
transform:translate(-50%, -50%) scale(1);
-webkit-transform:translate(-50%, -50%) scale(1);
}


