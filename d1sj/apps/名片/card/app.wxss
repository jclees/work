view,cover-view {
    box-sizing: border-box;
}

.clearfix:before,.clearfix:after {
    height: 0;
    width: 0;
    clear: both;
    content: "";
    margin: 0;
    padding: 0;
    display: block;
    visibility: hidden;
}

.scroll_box {
    transition: all 0.5s ease-in-out;
}

image {
    height: 100%;
    width: 100%;
    display: block;
}

